+    symlink = "uio/pwmss1/qep";
+
+    count_mode = <0>;
+    swap_inputs = <0>;
+    invert_qa = <0>;
+    invert_qb = <0>;
+    invert_qi = <0>;
+    invert_qs = <0>;
