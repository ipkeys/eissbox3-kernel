+            DRA7XX_CORE_IOPAD(0x3738, PIN_OUTPUT_PULLDOWN | MUX_MODE3 ) /* A21 mcasp4_fsx    uart8_txd  */
+        >;
+    };
+};
+
+&epwmss1 {
+    status = "okay";
+    ranges = <0x48440000 0x48440000 0x1000>;
